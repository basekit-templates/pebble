
{% extends "layout.twig" %}

{% block content %}

<div class="nav-extended wrapper">
    {{widget('extendednavigation', 'navigationheader', {})|raw}}
</div>

<header>
    <div class="wrapper clearfix">
            {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
        
        <div class="companyheader clearfix">   
            <div class="header-companyname">
                {{widget('companyname', 'companyname', {"business": "profile"})|raw}}
            </div><!-- end header-companyname -->
            <div class="header-search">
                {{widget('blogsearch', 'blogsearch', {})|raw}}
            </div><!-- end header-search -->
            
        </div>  <!--end companyheader-->  

    </div><!--end wrapper clearfix-->
</header>

<div class="main wrapper clearfix">
    <div class="canvas">
    {{zone('main')|raw}}
    </div>
</div> 

<footer>
    <div class="wrapper clearfix">
        {{widget('content', 'footercolumn1content', {"content":"Pulvinar Vitae"})|raw}}
        {{widget('profile', 'footercolumn1socialicons', { 'profileType' : 'socialicons' })|raw}}
    </div>
</footer>

{% endblock %}
