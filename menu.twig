
{% extends "layout.twig" %}

{% block content %}

<header>
    <div class="wrapper clearfix">
        <div class="header-left">
                {{widget('content', 'headercontentleft', {"content":"<p>Lorem ip, dolor sit amet consectet</p>"})|raw}}
            <div class="header-img">
                {{widget('image', 'imageleft', { 'src': asset('images/img-left.png'), 'title': '', 'alt': '', 'fileName': 'img-left.png', 'fileSize': '58kb', 'imageSize': '275 x 152', 'imageWidth': '100%'})|raw}}
            </div>
        </div> 

        <div class="companyheader clearfix">
            {% if profile.logo|length > 0 %}
                {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
            {% else %}
                {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
                {{widget('companyname', 'companyname', {"business": "profile"})|raw}}
            {% endif %}
        </div>
        <div class="header-right">
            {{widget('content', 'headercontentright', {"content":"<p>Lorem ip, dolor sit amet consectet</p>"})|raw}}
            <div class="header-img">
            {{widget('image', 'imageright', { 'src': asset('images/img-right.png'), 'title': '', 'alt': '', 'fileName': 'img-right.png', 'fileSize': '58kb', 'imageSize': '275 x 152', 'imageWidth': '100%'})|raw}}
            </div>
        </div>
    </div><!--end wrapper clearfix-->
</header>

<div class="main wrapper clearfix">
{{widget('menusections', 'menusections1', {})|raw}}
{{widget('menu', 'menu1', {})|raw}}
</div>

<footer>
    <div class="wrapper clearfix">
        {{widget('profile', 'footercolumn1socialicons', { 'profileType':'socialicons', 'socialicons':'profile'})|raw}}
    </div>
</footer>

{% endblock %}
